void setup() {
  // put your setup code here, to run once:
pinMode{7,OUTPUT};
}

void loop() {
  // put your main code here, to run repeatedly:
digitalWrite{7,HIGH]; //Pada pin 7, LED menyala
delay{4000}; //Tunda 4 sec pada saat nyala
digitalWrite{7,LOW};Pada pin 7, LED mati
delay{4000}; //tunda 4 sec pada saat mati
}
